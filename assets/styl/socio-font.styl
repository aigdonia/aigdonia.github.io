@charset "UTF-8";

@font-face {
  font-family: "Socio";
  src:url("fonts/socio.eot");
  src:url("fonts/socio.eot?#iefix") format("embedded-opentype"),
    url("fonts/socio.woff") format("woff"),
    url("fonts/socio.ttf") format("truetype"),
    url("fonts/socio.svg#socio") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "socio" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "Socio" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-facebook:before {
  content: "\61";
}
.icon-linkedin:before {
  content: "\62";
}
.icon-twitter:before {
  content: "\63";
}
.icon-github:before {
  content: "\65";
}
